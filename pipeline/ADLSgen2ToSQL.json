{
	"name": "ADLSgen2ToSQL",
	"properties": {
		"activities": [
			{
				"name": "csvToSQL",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "AzureBlobFSSource",
						"recursive": true
					},
					"sink": {
						"type": "AzureSqlSink"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"columnMappings": {
							"SystemInformationID": "SystemInformationID",
							" Database Version": "Database Version",
							" VersionDate": "VersionDate",
							" ModifiedDate": "ModifiedDate"
						}
					}
				},
				"inputs": [
					{
						"referenceName": "LoadcsvfromADLSgen2_fixed",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "InsertcvstoSQLDB_fixed",
						"type": "DatasetReference"
					}
				]
			}
		],
		"annotations": []
	}
}